<script type="text/javascript" defer>
$(document).ready(function(){
  {{#each albums}}
    new jPlayerPlaylist({
      jPlayer: "#jquery_jplayer_{{id}}",
      cssSelectorAncestor: "#jp_container_{{id}}"
    }, [
      {{#if audioOneName}}
      {
        title:"{{audioOneName}}",
        mp3:"audio/{{audioOneMp3.filename}}",
        oga:"audio/{{audioOneMp3.filename}}"
      }
      {{/if}}
      {{#if audioTwoName}}
      ,{
        title:"{{audioTwoName}}",
        mp3:"audio/{{audioTwoMp3.filename}}",
        oga:"audio/{{audioTwoMp3.filename}}"
      }
      {{/if}}
    ], {
      swfPath: "../js",
      supplied: "mp3, oga",
      wmode: "window",
      smoothPlayBar: true,
      keyEnabled: true
    });
    {{/each}}
  });
</script>
